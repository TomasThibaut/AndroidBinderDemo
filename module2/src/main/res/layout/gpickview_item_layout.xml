<layout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
>

	<data>
		<variable name="text" type="String"/>
	</data>
	<RelativeLayout
			android:layout_width="35dp"
			android:layout_height="50dp">
		<TextView
				tools:text="09"
